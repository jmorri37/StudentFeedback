CREATE TABLE IF NOT EXISTS Student login (
   sID serial PRIMARY KEY, 
   firstname VARCHAR(50) NOT NULL,
   lastname VARCHAR(50) NOT NULL,
   email adress VARCHAR(100) NOT NULL,
  
   status Boolean NOT NULL,
   

